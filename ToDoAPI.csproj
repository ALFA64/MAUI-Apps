    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>67600f0e-9bfb-4634-b8f8-e79a54fc2237</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="9.0.1">
